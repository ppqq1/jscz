# 使用 Python 定义变量并输出变量的值
name = Alice
age = 21
print(My name is, name, and I am, age, years old)

// 使用 C++ 定义变量并输出变量的值
#include <iostream>
using namespace std;
int main() {
  string name = Alice;
  int age = 21;
  cout << My name is  << name <<  and I am  << age <<  years old << endl;
  return 0;
}
